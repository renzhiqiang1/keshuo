<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="cacheEnabled" value="true" /><!-- 全局映射器启用缓存 -->
        <setting name="useGeneratedKeys" value="true" />
        <setting name="defaultExecutorType" value="REUSE" />
        <!-- <setting name="logImpl" value="STDOUT_LOGGING" />打印SQL语句 -->
        <setting name="jdbcTypeForNull" value="NULL" /><!-- JDBC类型为空时设置默认值 -->
    </settings>
    <typeAliases>
        <typeAlias alias="Integer" type="java.lang.Integer" />
        <typeAlias alias="Long" type="java.lang.Long" />
        <typeAlias alias="HashMap" type="java.util.HashMap" />
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap" />
        <typeAlias alias="ArrayList" type="java.util.ArrayList" />
        <typeAlias alias="LinkedList" type="java.util.LinkedList" />

        <typeAlias alias="TbUpToDownInfoEntity" type="com.ks.keshuoservice.entity.Interface.TbUpToDownInfoEntity" />
        <typeAlias alias="TbUpInfoEntity" type="com.ks.keshuoservice.entity.Interface.TbUpInfoEntity" />
        <typeAlias alias="TbResultInfoEntity" type="com.ks.keshuoservice.entity.Interface.TbResultInfoEntity" />
        <typeAlias alias="pd" type="com.ks.keshuoservice.utils.common.PageData" />
        <typeAlias alias="TbUpManangeInfoEntity" type="com.ks.keshuoservice.entity.Redirect.TbUpManangeInfoEntity" />
        <typeAlias alias="TbUpManangeParamInfoEntity" type="com.ks.keshuoservice.entity.Redirect.TbUpManangeParamInfoEntity" />
        <typeAlias alias="TbDownManageInfoEntity" type="com.ks.keshuoservice.entity.Redirect.TbDownManageInfoEntity" />
        <typeAlias alias="TbMappingInfoEntity" type="com.ks.keshuoservice.entity.Redirect.TbMappingInfoEntity" />
        <typeAlias alias="TbDownMappingInfoEntity" type="com.ks.keshuoservice.entity.Redirect.TbDownMappingInfoEntity" />
    </typeAliases>







</configuration>